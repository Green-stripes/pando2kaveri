<h1>Your readings:</h1>
ROOM 4 <%= @data_by_room[4][:levels_CO2]%>
ROOM 1 <%= @data_by_room[1][:levels_CO2]%>

<div class="cards" data-controller="graphics">
  <div class="card-category">
  <canvas data-graphics-target="myChart" width="400" height="400" data-graphics-chart-data="<%= {
    labels: @data_by_room[4][:times_CO2],
    data_CO2: @data_by_room[4][:levels_CO2],
    data_TMP: @data_by_room[4][:levels_TMP],
    data_VOCT: @data_by_room[4][:levels_VOCT],
    data_HUM: @data_by_room[4][:levels_HUM],
  }.to_json %>"></canvas>
</div>
